library(leaflet)
library(htmlwidgets)
library(dplyr)

df <- read.csv("CA.csv")
sam.df <- sample_n(df, 60000)


m <- leaflet() %>%
  setView(lat = 36.7783, lng = -119.4179, zoom = 6) %>% 
  addTiles() %>% 
  addCircleMarkers(data = sam.df, lng = ~Start_Lng, lat = ~Start_Lat, 
                   popup = ~Street, radius = 0.5,
                   clusterOptions = markerClusterOptions())

saveWidget(m, file = "clusters.html")
